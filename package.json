{
  "name": "fe-react-poc-calendar",
  "version": "1.0.0",
  "license": "MIT",
  "description": "A modern React calendar application with multiple views and event management",
  "keywords": [
    "React",
    "Calendar",
    "styled-components",
    "TypeScript",
    "Frontend",
    "Vite",
    "Modern Design",
    "Dark Mode",
    "Ant Design",
    "Redux Toolkit",
    "Persistent Storage",
    "Import/Export"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/5h1ngy/fe-react-poc-calendar.git"
  },
  "contributors": [
    {
      "name": "5h1ngy",
      "url": "https://github.com/5h1ngy"
    },
    {
      "name": "Davide1786",
      "url": "https://github.com/Davide1786"
    }
  ],
  "author": "5h1ngy",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "type": "module",
  "scripts": {
    "start:mock": "vite --mode mock",
    "start:dev": "vite --mode development",
    "build:prod": "tsc -b && vite build --mode prod --outDir dist",
    "preview:prod": "vite preview --mode prod --outDir dist",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "clean": "rimraf dist node_modules",
    "docker:build": "docker build -t fe-react-poc-calendar:latest .",
    "docker:run": "docker run -p 8080:80 fe-react-poc-calendar:latest",
    "docker:build-run": "npm run docker:build && npm run docker:run",
    "docker:push": "docker tag fe-react-poc-calendar:latest 5h1ngy/fe-react-poc-calendar:latest && docker push 5h1ngy/fe-react-poc-calendar:latest"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "@types/redux-persist": "^4.3.1",
    "axios": "^1.6.2",
    "i18next": "^23.10.1",
    "i18next-browser-languagedetector": "^7.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.3",
    "react-i18next": "^14.1.0",
    "react-icons": "^4.12.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.20.1",
    "redux-persist": "^6.0.0",
    "styled-components": "^6.1.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "axios-mock-adapter": "^1.22.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "packageManager": "yarn@4.8.1"
}
